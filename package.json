{
  "name": "ai-memory-system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --config jest.config.ci.ts --ci --coverage --maxWorkers=2",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:update": "jest -u",
    "test:clear": "jest --clearCache",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prepare": "husky install",
    "push:skip-checks": "SKIP_PRE_PUSH=true git push",
    "pre-commit": "npm run lint && npm run type-check && npm test",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "pre-push": "npm run check-workflow-compliance && npm run check-documentation",
    "check-workflow-compliance": "ts-node scripts/check-workflow-compliance.ts",
    "check-documentation": "ts-node scripts/check-documentation.ts",
    "check-coverage": "jest --config jest.config.ci.ts --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 3000",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook --maxWorkers=2",
    "percy:snapshot": "percy snapshot storybook-static",
    "build-storybook": "storybook build",
    "generate-favicon": "node scripts/generate-favicon.js"
  },
  "dependencies": {
    "@prisma/client": "^5.11.0",
    "ai": "^2.2.37",
    "dotenv": "^16.4.7",
    "next": "^14.1.3",
    "next-themes": "^0.2.1",
    "openai": "^4.89.0",
    "react": "^18.2.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@faker-js/faker": "^8.4.1",
    "@storybook/addon-a11y": "^8.6.8",
    "@storybook/addon-coverage": "^1.0.5",
    "@storybook/addon-docs": "^8.6.8",
    "@storybook/addon-essentials": "^8.6.8",
    "@storybook/addon-interactions": "^8.6.8",
    "@storybook/addon-links": "^8.6.8",
    "@storybook/addon-onboarding": "^8.6.8",
    "@storybook/addon-styling-webpack": "^1.0.1",
    "@storybook/addon-themes": "^8.6.8",
    "@storybook/blocks": "^8.6.8",
    "@storybook/nextjs": "^8.6.8",
    "@storybook/react": "^8.6.8",
    "@storybook/test": "^8.6.8",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.25",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "glob": "^10.3.10",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.5.0",
    "lucide-react": "^0.483.0",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "prisma": "^5.11.0",
    "react-dom": "^18.3.1",
    "storybook": "^8.6.8",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
